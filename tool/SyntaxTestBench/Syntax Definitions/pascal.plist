[ 	]*(([Pp][Rr][Oo][Cc][Ee][Dd][Uu][Rr][Ee])|([Ff][Uu][Nn][Cc][Tt][Ii][Oo][Nn])|([Oo][Pp][Ee][Rr][Aa][Tt][Oo][Rr])|((([Cc][Oo][Nn])|([Dd][Ee]))[Ss][Tt][Rr][Uu][Cc][Tt][Oo][Rr]))[[:space:]]+[A-Za-z0:=*/+-9_.]+[[:space:]]*(\([^)]*\))?[[:space:]]*(([A-Za-z0-9_]+[[:space:]]*)?:[[:space:]]*[A-Za-z_][A-Za-z0-9_]*)?;</string>
