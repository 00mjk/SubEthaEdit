<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dictionary SYSTEM "file://localhost/System/Library/DTDs/sdef.dtd">

<dictionary>
    <suite name="SubEthaEdit Suite" code="Hdra">
        <types>
			<enumeration name="access options" code="Acco">
                <enumerator name="read write" code="RdWr" description="read/write"/>
                <enumerator name="read only" code="RdOn" description="read only"/>
                <enumerator name="locked" code="Lock" description="locked"/>
			</enumeration>        
		</types>
        <classes>
            <class name="application" code="capp" inherits="application">
                <cocoa class="NSApplication"/>
                <elements>
                    <element type="text document">
                        <cocoa method="orderedDocuments"/>
                    </element>
                </elements>
				<responds-to-commands>
					<responds-to name="SubEthaEditSuite.Open">
						<cocoa method="TCM_handleOpenScriptCommand:"/>
					</responds-to>
					<responds-to name="SubEthaEditSuite.Print">
						<cocoa method="TCM_handlePrintScriptCommand:"/>
					</responds-to>
					<responds-to name="see">
						<cocoa method="TCM_handleSeeScriptCommand:"/>
					</responds-to>
				</responds-to-commands>
            </class>
            <class name="text document" code="pltd" inherits="document">
                <cocoa class="PlainTextDocument"/>
                <contents name="contents" code="ctxt" type="text" description="the text of the document">
                    <cocoa method="textStorage"/>
                </contents>
                <properties>
                    <property name="text" code="ctxt" type="text" description="the text of the document">
                        <cocoa method="textStorage"/>
                    </property>
                    <property name="encoding" code="Encd" type="string" description="the internet name of the document's encoding">
                        <cocoa method="encoding"/>
                    </property>
                    <property name="mode" code="Mode" type="string" description="the name of the document's mode">
                        <cocoa method="mode"/>
                    </property>
                    <property name="access control" code="Actl" type="access options" description="the access right of the document">
                        <cocoa method="accessOption"/>
                    </property>
                    <property name="announced" code="Anno" type="boolean" description="is the document announced?">
                        <cocoa method="isAnnounced"/>
                    </property>
                    <property name="URL" code="DURL" type="string" access="r" description="the URL of the document">
                        <cocoa method="announcementURL"/>
                    </property>
                </properties>
				<responds-to-commands>
                    <responds-to name="SubEthaEditSuite.Print">
						<cocoa method="handlePrintScriptCommand:"/>
					</responds-to>
				</responds-to-commands>
            </class>
            <class name="window" code="cwin" inherits="window">
				<cocoa class="NSWindow"/>
				<responds-to-commands>
					<responds-to name="SubEthaEditSuite.Print">
						<cocoa method="handlePrintScriptCommand:"/>
					</responds-to>
                </responds-to-commands>
            </class>                
            <class name="text" plural="text" code="ctxt" description="Text">
				<cocoa class="TextStorage"/>
				<elements>
					<element type="paragraph">
						<cocoa method="paragraphs"/>
					</element>
					<element type="word">
						<cocoa method="words"/>
					</element>
					<element type="character">
						<cocoa method="characters"/>
					</element>
					<element type="attribute run">
						<cocoa method="attributeRuns"/>
					</element>
					<!-- <element type="attachment">
						<cocoa method="attachments"/>
					</element> -->
				</elements>
                <properties>
					<property name="color" code="colr" type="color" access="r" description="The color of the first character.">
						<cocoa method="foregroundColor"/>
					</property>
					<property name="font" code="font" type="string" access="r" description="The name of the font of the first character.">
						<cocoa method="fontName"/>
					</property>
					<!-- <property name="size" code="ptsz" type="number" description="The size in points of the first character.">
						<cocoa method="fontSize"/>
					</property> -->
				</properties>
			</class>
            <class name="paragraph" code="cpar" inherits="text" description="This subdivides the text into paragraphs.">
				<cocoa class="TextStorage"/>
			</class>
			<class name="word" code="cwor" inherits="text" description="This subdivides the text into words.">
				<cocoa class="TextStorage"/>
			</class>
			<class name="character" code="cha " inherits="text" description="This subdivides the text into characters.">
				<cocoa class="TextStorage"/>
			</class>
			<class name="attribute run" code="catr" inherits="text" description="This subdivides the text into chunks that all have the same attributes.">
				<cocoa class="TextStorage"/>
			</class>
        </classes>
        <commands>
			<command name="open" code="aevtodoc" description="Open an object.">
				<direct-parameter type="file" description="The file(s) to be opened."/>
                <parameter name="with properties" code="prdt" type="record" optional="optional" description="the initial values for the properties, to be applied to the document when opening">
                    <cocoa key="WithProperties"/>
                </parameter>			
            </command>
			<command name="print" description="Print an object." code="aevtpdoc">
				<!-- type would be better written as "file | document". -->
				<direct-parameter type="file" description="The file(s) or document(s) to be printed."/>
                <parameter name="with properties" code="prdt" type="record" optional="optional" description="the initial values for the properties, to be applied to the document when printing">
                    <cocoa key="WithProperties"/>
                </parameter>
			</command>
            <command name="get URL" code="GURLGURL" description="Open a URL.">
                <synonyms>
					<synonym name="get url"/>
				</synonyms>
                <cocoa class="GetURLCommand"/>
                <direct-parameter type="object" description="The URL to open."/>
            </command>
            <command name="open URL" code="WWW!OURL" description="Open a URL.">
                <synonyms>
					<synonym name="open url"/>
				</synonyms>
                <cocoa class="GetURLCommand"/>
                <direct-parameter type="object" description="The URL to open."/>
            </command>
            <command name="see" code="HdraSee " hidden="hidden" description="">
                <result type="file" description=""/>
                <parameter name="files" code="File" type="string" optional="optional" description="">
					<cocoa key="Files"/>
				</parameter>                
                <parameter name="new files" code="NuFl" type="string" optional="optional" description="">
					<cocoa key="NewFiles"/>
				</parameter>                
                <parameter name="stdin" code="Stdi" type="string" optional="optional" description="">
					<cocoa key="Stdin"/>
				</parameter>
                <parameter name="jobdescription" code="JobD" type="string" optional="optional" description="">
					<cocoa key="JobDescription"/>                    
                </parameter>
                <parameter name="pipetitle" code="Pipe" type="string" optional="optional" description="">
					<cocoa key="PipeTitle"/>                    
                </parameter>
                <parameter name="print" code="Prnt" type="boolean" optional="optional" description="">
					<cocoa key="ShouldPrint"/>                    
                </parameter>
                <parameter name="wait" code="Wait" type="boolean" optional="optional" description="">
 					<cocoa key="ShouldWait"/>                   
                </parameter>
                <parameter name="pipeout" code="Wait" type="boolean" optional="optional" description="">
 					<cocoa key="PipeOut"/>                   
                </parameter>
                <parameter name="with properties" code="prdt" type="record" optional="optional" description="initial properties">
                    <cocoa key="WithProperties"/>
                </parameter>
            </command>
        </commands>
    </suite>
</dictionary>